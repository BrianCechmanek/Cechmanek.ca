/* CSS sheet TO DO */

.nav {
 background-color: black;
 height: 50px;
 width: 100%;
}

.nav li {
 display: inline;
 text-transform: uppercase;
 font-size: 16px;
 font-family: Monaco;
 font-weight: bold;
 color: white;
 padding-left: -20px;
 padding-right: 20px;
}

#top.jumbotron {
 background-image:url("img/BlackSkyTop.png");
 height: 500px;
 background-size: 100%;
 background-repeat: no-repeat;
 padding-bottom: 0px;
 margin-bottom: 0px;
}

.block {
    background-color: black;
    height: 150px;
    width: 100%;
    padding-top: 0px;
    margin: 0px;
    float: clear;
}

#mid.jumbotron {
    background-image:url("img/BlackSkyMid.png");
    height:600px;
    background-size: 100%;
    bakground-repeat: no-repeat;
    margin: 0px;
}

#bot.jumbotron {
    background-image:url("img/BlackSkyBot.png");
    height: 500px;
    background-size: 100%;
    background-repeat: no-repeat;
}

#footer {
    background-color: #1F1F14;
    height: 50px;
    width: 95%;
    padding: 5px 5px 5px 5px;
    border-radius: 10px;
    clear: both; 
}


/* text styling goes below */
